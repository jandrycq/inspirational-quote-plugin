{
  "name": "otakupahp/jandry-onboard-test",
  "description": "Onboard test for Jandry",
  "license": "GPL-2.0-or-later",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/a8cteam51/team51-configs"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-json": "*"
  },
  "require-dev": {
    "a8cteam51/team51-configs": "dev-trunk",
    "wp-coding-standards/wpcs": "^3",
    "roave/security-advisories": "dev-latest"
  },
  "scripts": {
    "packages-install": "composer install",
    "lint:php": "phpcs . || [ $? -eq 3 ] && exit 0"
  },
  "config": {
    "allow-plugins": {
      "composer/*": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
